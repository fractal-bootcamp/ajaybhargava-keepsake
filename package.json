{
  "name": "keepsake",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "database": "docker run --name drizzle-postgres -e POSTGRES_PASSWORD=mypassword -d -p 10241:5432 postgres",
    "database:stop": "docker stop drizzle-postgres",
    "database:kill": "docker rm drizzle-postgres",
    "server": "bun src/index.ts"
  },
  "dependencies": {
    "@clerk/react-router": "^1.0.8",
    "@elysiajs/eden": "^1.2.0",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.3",
    "elysia": "^1.2.12",
    "isbot": "^5.1.17",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5"
  },
  "devDependencies": {
    "@netlify/vite-plugin-react-router": "^1.0.0",
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.0",
    "@types/bun": "^1.2.2",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "drizzle-kit": "^0.30.4",
    "react-router-devtools": "^1.1.0",
